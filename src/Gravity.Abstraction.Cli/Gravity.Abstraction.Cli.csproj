<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>net6.0</TargetFrameworks>
		<PackageId>Gravity.Abstraction.Cli</PackageId>
		<LangVersion>latest</LangVersion>
		<Title>Gravity API - CLI Abstraction</Title>
		<Authors>Gravity-API Community</Authors>
		<Description>CLI (command line integration) parser, for reading Gravity formatted text commands.</Description>
		<PackageTags>automation,crawling,crawl,selenium,appium,automation testing,automation test,qa,gravity,gravity api,scrapping,scrap</PackageTags>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/gravity-api/gravity-abstraction-cli</PackageProjectUrl>
		<NeutralLanguage>en-US</NeutralLanguage>
	</PropertyGroup>
</Project>
